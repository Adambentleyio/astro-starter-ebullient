---
import Container from "@components/container.astro";
import Hero from "@components/hero.astro";
import Layout from "@layouts/Layout.astro";
import heroGym from "@assets/hero-gym.png"



  const TemplateContent = {
    hero: {
        title: "A brand new website service for awesome folks.",
        hook: "We're a small team of designers and developers who can help you build your next website.",
        bulletList: [
            "Personal Trainers",
            "Therapists",
            "Yoga Instructors",
            "Exective Coaches",
        ],
        image: {
            src: heroGym,
            alt: "hero gym",
            width: 480,
            height: 560,
        },
    }
}
---

<Layout title="">
  <Container>
    <Hero props={TemplateContent.hero} />
  </Container>
</Layout>
